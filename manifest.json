{
    "name": "One dark theme for Quercus",
    "description": "a one dark like theme for University of Toronto's student website Quercus.",
    "version": "1.1",
    "icons": { "128": "/images/128.png" },
    "manifest_version": 3,
      "permissions":  [
      ],
      "action": {
        "default_icon": {
            "128": "/images/128.png"
          }
      },
      "content_scripts": [
        {
          "matches": ["https://q.utoronto.ca/*"],
          "css": ["one-dark.css"]
        }
      ]
      
  }